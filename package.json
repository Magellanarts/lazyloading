{
  "name": "lazyloading",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "lint": "vue-cli-service lint",
    "test": "jest",
    "lambda-serve": "netlify-lambda serve lambda-src",
    "lambda-build": "NODE_ENV=production webpack && netlify-lambda build lambda-src",
    "lambda-dev": "NODE_ENV=development concurrently \"webpack-dev-server --content-base src/\" \"yarn run lambda-serve\""
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.17",
    "@fortawesome/free-solid-svg-icons": "^5.8.1",
    "@fortawesome/vue-fontawesome": "^0.1.6",
    "@voerro/vue-tagsinput": "^1.11.2",
    "add": "^2.0.6",
    "algoliasearch": "^3.32.1",
    "axios": "^0.18.0",
    "dotenv": "^8.0.0",
    "firebase": "^5.8.5",
    "google-maps": "^3.3.0",
    "moment": "^2.24.0",
    "netlify-lambda": "^1.4.11",
    "sparkpost": "^2.1.3",
    "stripe": "^7.0.0",
    "uuid": "^3.3.2",
    "vue": "^2.6.6",
    "vue-instantsearch": "^2.0.1",
    "vue-moment": "^4.0.0",
    "vue-router": "^3.0.1",
    "vue-wysiwyg": "^1.7.2",
    "vue2-google-maps": "^0.10.6",
    "vue2-touch-events": "^2.0.0",
    "vuefire": "^2.0.0-alpha.20",
    "vuex": "^3.0.1",
    "vuexfire": "^3.0.0-alpha.14",
    "webpack": "^4.31.0",
    "yarn": "^1.15.2"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.4.0",
    "@vue/cli-plugin-eslint": "^3.4.0",
    "@vue/cli-service": "^3.4.0",
    "@vue/eslint-config-airbnb": "^4.0.0",
    "@vue/test-utils": "^1.0.0-beta.29",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "eslint": "^5.8.0",
    "eslint-plugin-vue": "^5.0.0",
    "jest": "^24.8.0",
    "node-sass": "^7.0.0",
    "sass-loader": "^7.1.0",
    "vue-jest": "^3.0.4",
    "vue-template-compiler": "^2.5.21"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest"
    },
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    }
  }
}
